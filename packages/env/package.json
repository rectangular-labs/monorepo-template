{
  "name": "@rectangular-labs/env",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/env.d.ts",
      "default": "./src/env.ts"
    }
  },
  "scripts": {
